    echo
    echo " A command line command to tar up results of mvBestBET"
    echo
    . ${thisDir}/auxillary/help/help_options_start

    echo 
    echo " e.g. "
    echo
    echo "     tarBET 050126zz"
    echo
    echo
    echo " Current command default configuration is:"
    echo 

    . ${thisDir}/auxillary/help/help_tarBET_Options
