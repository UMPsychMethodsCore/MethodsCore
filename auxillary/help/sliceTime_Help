    echo
    echo "A command line interface to slice time correction procedures in FSL."
    echo "This uses the FSL routines : slicetimer."
    echo 
    echo
    echo
    . ${thisDir}/auxillary/help/help_options_start

    echo
    echo " e.g."
    echo 
    echo "  To slice-time correct fmri volumes in runs 1,2,4 you would:"
    echo 
    echo "     sliceTime -v prun -f func/MSIT -s img/ -n 10 -r 1 -r 2 -r 4 060402rw"
    echo 
    echo "  To realign fmri volumes in inclusive range of 1-10, do:"
    echo
    echo "     sliceTime -v prun -f func/MSIT -s img/ -n 10 -i 1-10 060402rw"
    echo 
    echo " Current command default configuration is:"
    echo 
    echo
    
    . ${thisDir}/auxillary/help/help_${thisCommand}_Options

