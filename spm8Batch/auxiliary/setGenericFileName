		  shift
		  let argn++
		  if (( $argn >= $args ))
		      then
		      echo "Missing parameter for overlay name, aborting"
	              . ${thisDir}/auxiliary/exit_w_removal
		  else
		      FILENAME=$1
		  fi
		  . ${thisDir}/fileExtensionDetermination
		  if [ ${BAD} == "1" ] 
		  then
		      echo
		      echo "We can not process ANALYZE img/hdr data, we only support NIFTI (.nii or .nii.gz)"
		      echo
		      echo "ABORTING"
		      echo
	              . ${thisDir}/auxiliary/exit_w_removal
		  fi
