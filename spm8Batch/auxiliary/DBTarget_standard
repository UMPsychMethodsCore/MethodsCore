		  #
		  # Set the error code to bad parameter.
		  #
		  SPM8BATCH_ERROR_CODE=64
		  #
		  shift
		  let argn++
		  if (( $argn >= $args ))
		      then
		      echo "Missing parameter for DBTarget quick code, aborting"
	              . ${thisDir}/auxiliary/exit_w_removal
		  else
		      echo "DBTarget specification by quick code not yet supported."
		      echo "Consider using custom URL instead (-Y flag)"
	              . ${thisDir}/auxiliary/exit_w_removal   
		  fi
		  # 
		  # Change back to success
		  #
		  SPM8BATCH_ERROR_CODE=0
