    echo
    echo "A command line interface to slice time correction procedures in FSL."
    echo "This uses the FSL routines : slicetimer."
    echo 
    echo
    echo
    . ${thisDir}/auxiliary/help/help_options_start

    echo
    echo " e.g."
    echo 
    echo "  To slice-time correct fmri volumes you would:"
    echo 
    echo "     sliceTime8 -v prun -f func/MSIT 060402rw"
    echo 
    echo " Current command default configuration is:"
    echo 
    echo
    
    . ${thisDir}/auxiliary/help/help_${thisCommand}_Options

