    echo
    echo " A command line command to drive batch processing to coregister the 'HIRES'"
    echo " to 'overlay' analyze image. "
    echo
    . ${thisDir}/auxiliary/help/help_options_start

    echo 
    echo " e.g. "
    echo
    echo "   To coregister the 'het1spgr.img' to the 'het1overlay.img' that has been coreg'd"
    echo "   to the fMRI times series and to drop the results in 'func/MSIT/coReg' :"
    echo 
    echo "     coregHiRes -w MSIT/coReg 050126zz"
    echo
    echo
    echo " Current command default configuration is:"
    echo 

    . ${thisDir}/auxiliary/help/help_coregHiRes_Options
