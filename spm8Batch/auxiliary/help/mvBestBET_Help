    echo
    echo " A command line command to move the best BET to generic name "
    echo " for further spm8 pre-processing. The file will be called : "	
    echo 
    echo "      [anatomy directory]/BET/bets_BET_hiRes.nii"
    echo
    . ${thisDir}/auxiliary/help/help_options_start

    echo 
    echo " e.g. "
    echo
    echo "     mvBestBET 050126zz"
    echo
    echo
    echo " Current command default configuration is:"
    echo 

    . ${thisDir}/auxiliary/help/help_mvBestBET_Options
