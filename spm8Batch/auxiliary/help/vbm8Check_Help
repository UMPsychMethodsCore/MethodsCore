    echo
    echo " A command line command to fslview the ht1spgr and vbm8 bet"
    echo
    . ${thisDir}/auxiliary/help/help_options_start

    echo 
    echo " e.g. "
    echo
    echo "     vbm8Check 050126zz"
    echo
