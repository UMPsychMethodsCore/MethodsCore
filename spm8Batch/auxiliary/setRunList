		  shift 
		  let argn++
		  if (( $argn >= $args ))
		  then
		      echo "Missing run list option, abortin."
	              . ${thisDir}/auxiliary/exit_w_removal
		  else
		      runLIST=$1
		      run0=`echo ${runLIST} | awk -F- '{print $1}'`
		      runX=`echo ${runLIST} | awk -F- '{print $2}'`
		      runNAMES=(`for (( ii=$run0 ; ii<= $runX ; ii++ )) do echo $ii ; done`)
		      let nRUN=`expr ${#runNAMES[@]}`
		  fi
